[project]
	description = server and android app
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/sepehr1
[access "refs/for/refs/*"]
	read = group user/sepehr1
	push = group user/sepehr1
	label-Code-Review = -2..+2 group user/sepehr1
	label-Verified = -1..+1 group user/sepehr1
	rebase = group user/sepehr1
	removeReviewer = group user/sepehr1
	submit = group user/sepehr1
[access "refs/heads/*"]
	read = group user/sepehr1
	create = group user/sepehr1
	push = +force group user/sepehr1
	pushMerge = group user/sepehr1
[access "refs/tags/*"]
	push = group user/sepehr1
	push = +force group user/sepehr1
